{
    "name": "argo-ui",
    "version": "2.1.0-alpha7",
    "main": "./bundle/index.js",
    "types": "./bundle/lib/index.d.ts",
    "license": "MIT",
    "files": [
        "src/assets",
        "bundle"
    ],
    "scripts": {
        "docker": "./scripts/build_docker.sh",
        "build": "yarn build:ui && yarn build:api",
        "build:ui": "webpack --config ./src/app/webpack.config.js",
        "build:api": "tsc -p ./src/api/tsconfig.json",
        "dev": "nf start",
        "dev:ui": "webpack-dev-server --config ./src/app/webpack.config.js",
        "dev:api": "TS_NODE_PROJECT=./src/api/tsconfig.json nodemon --nolazy --inspect -r ts-node/register ./src/api/main.ts",
        "lint": "yarn lint:ui && yarn lint:api",
        "lint:ui": "tslint -p ./src/app",
        "lint:api": "tslint -p ./src/api",
        "utils:icons": "rm -f src/assets/fonts/* && node ./scripts/icons/generator.js",
        "test": "mocha --require ts-node/register ./src/app/**/*.spec.ts",
        "prepublishOnly": "tsc -p ./src/lib/tsconfig.json && webpack --config ./src/lib/webpack.config.js"
    },
    "dependencies": {
        "aws-sdk": "^2.188.0",
        "body-parser": "^1.18.2",
        "express": "^4.16.2",
        "json-stream": "^1.0.0",
        "kubernetes-client": "3.17.1",
        "moment": "^2.20.1",
        "rxjs": "^5.5.6",
        "ws": "^4.0.0",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "@types/aws-sdk": "^2.7.0",
        "@types/chai": "^4.1.2",
        "@types/classnames": "^2.2.3",
        "@types/dagre": "^0.7.39",
        "@types/deep-equal": "^1.0.1",
        "@types/history": "^4.6.2",
        "@types/mocha": "^2.2.48",
        "@types/prop-types": "^15.5.2",
        "@types/react": "^16.0.34",
        "@types/react-dom": "^16.0.3",
        "@types/react-redux": "^5.0.14",
        "@types/react-router-dom": "^4.2.3",
        "@types/react-router-redux": "^5.0.11",
        "@types/redux": "^3.6.0",
        "@types/superagent": "^3.5.7",
        "@types/ws": "^4.0.0",
        "@types/yamljs": "^0.2.30",
        "awesome-typescript-loader": "^3.4.1",
        "chai": "^4.1.2",
        "classnames": "^2.2.5",
        "copy-webpack-plugin": "^4.3.1",
        "dagre": "^0.8.2",
        "deep-equal": "^1.0.1",
        "font-awesome": "^4.7.0",
        "foreman": "^2.0.0",
        "foundation-sites": "^6.4.3",
        "glob": "^7.1.2",
        "history": "^4.7.2",
        "html-webpack-plugin": "^2.30.1",
        "jscs": "^3.0.7",
        "mocha": "^5.0.0",
        "node-sass": "^4.7.2",
        "nodemon": "^1.14.11",
        "prop-types": "^15.6.0",
        "raw-loader": "^0.5.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-hot-loader": "^3.1.3",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "redux": "3.6.0",
        "sass-loader": "^6.0.6",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.20.1",
        "superagent": "^3.8.2",
        "superagent-promise": "^1.1.0",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "tslint-react": "^3.4.0",
        "typescript": "^2.6.2",
        "webfonts-generator": "^0.4.0",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.11.1",
        "xterm": "2.4.0",
        "yamljs": "^0.3.0"
    }
}
